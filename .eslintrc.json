{
  "extends": ["prettier", "plugin:react-hooks/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "no-use-before-define": "off",
    "prettier/prettier": "error",
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off"
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  }
}
